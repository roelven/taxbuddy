{
	"config_version": "2",
	"platform_version": "v1.3",
	"name": "taxbuddy",
	"author": "Johan-robot@freenerd.de",
	"version": "0.1.4",
	"description": "FILE FUCKING TAXES FAST",
	"modules": {
		"activations": [
			{
				"patterns": ["http://*/*", "https://*/*"],
				"styles": [],
				"scripts": ["js/every-page.js"],
				"run_at": "start",
				"all_frames": false
			}
		],
    "icons": {
      "android": {
        "36": "img/icons/icon36.png",
        "48": "img/icons/icon48.png",
        "72": "img/icons/icon72.png"
      },
      "ios": {
        "57": "img/icons/icon57.png",
        "72": "img/icons/icon72.png",
        "114": "img/icons/icon114.png",
        "512": "img/icons/icon512.png"
      }
    },
		"orientations": {
		  "default": "portrait",
      "iphone": "portrait",
      "android": "portrait",
      "ipad": "portrait"
		},
    "launchimage": {
      "iphone": "img/iphone1.jpg",
      "iphone-retina": "img/iphone1.jpg",
      "ipad": "img/iphone1.jpg",
      "ipad-landscape": "img/iphone1.jpg",
      "android": "img/iphone1.jpg",
      "android-landscape": "img/iphone1.jpg"
    },
		"background": {
			"files": ["js/background.js"]
		},
		"button": {
			"default_title": "An example browser toolbar icon",
			"default_popup": "index.html",
			"default_icon": "img/logo-forge.png"
		},
		"contact": false,
		"event": true,
		"file": true,
		"geolocation": false,
		"is": true,
		"logging": {
			"level": "DEBUG"
		},
		"message": false,
		"notification": false,
		"prefs": true,
		"request": {
			"permissions": []
		},
		"sms": false,
		"tabs": true,
		"tools": true
	}
}
